        </rdfs:domain>
        <rdfs:range rdf:resource="http://qudt.org/schema/qudt#Datatype"/>
        <rdfs:comment xml:lang="en">Assigns a qudt:Datatype instance to an instance of a Value subclass or DataDimension</rdfs:comment>
        <rdfs:label xml:lang="en">Value Datatype</rdfs:label>
    </owl:ObjectProperty>
    


