        <foaf:logo>https://raw.githubusercontent.com/semts-ontology/SemTS/78047e27e691f5ac7878d93bec2f25ab514bd06d/assets/images/logo.png</foaf:logo>
        <foaf:maker>grass.alexan@gmail.com</foaf:maker>
        <schema:citation xml:lang="en">Alexander Graß, Rohit A. Deshmukh. The Semantic Time Series Ontology. https://github.com/semts-ontology/SemTS</schema:citation>
    </owl:Ontology>
    


