        <rdfs:comment>Explicit redefinition for a reuse in SemTS</rdfs:comment>
        <rdfs:label>Datatype</rdfs:label>
    </owl:Class>
    


    <!-- http://qudt.org/schema/qudt#Unit -->

    <owl:Class rdf:about="http://qudt.org/schema/qudt#Unit">
        <rdfs:comment>Explicit redefinition for a reuse in SemTS</rdfs:comment>
        <rdfs:label>Unit</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/2004/02/skos/core#Concept -->

    <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept">
        <rdfs:comment>Explicit redefinition for a reuse in SemTS</rdfs:comment>
        <rdfs:label>Concept</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/2006/time#DateTimeInterval -->

    <owl:Class rdf:about="http://www.w3.org/2006/time#DateTimeInterval">
