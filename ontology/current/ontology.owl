    <!-- https://w3id.org/semts/ontology#description -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/semts/ontology#description">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#DataDimension"/>
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#Knowledge"/>
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#KnowledgeGenerationEntity"/>
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#KnowledgeGenerationIO"/>
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#KnowledgeQualityMeasure"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Inherits from dc:description and can be used to optionally add a description to an entity of the domain.</rdfs:comment>
        <rdfs:label xml:lang="en">Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/semts/ontology#dimensionPosition -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/semts/ontology#dimensionPosition">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#DataDimension"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment xml:lang="en">Specifies the position of the associated dimension within a dataframe or similar ordered data structures.</rdfs:comment>
        <rdfs:label xml:lang="en">Dimension Position</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/semts/ontology#title -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/semts/ontology#title">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#DataDimension"/>
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#Knowledge"/>
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#KnowledgeGenerationEntity"/>
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#KnowledgeGenerationIO"/>
                    <rdf:Description rdf:about="https://w3id.org/semts/ontology#KnowledgeQualityMeasure"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Inherits from dc:title and can be used to optionally add a title to an entity of the domain.</rdfs:comment>
        <rdfs:label xml:lang="en">Title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/semts/ontology#valueString -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/semts/ontology#valueString">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/semts/ontology#EmbeddedValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The string representation for a concrete value of an Embedded Value. Complementary, the actual data type is defined via the dType property.</rdfs:comment>
        <rdfs:label xml:lang="en">Value String</rdfs:label>
    </owl:DatatypeProperty>
    


